{
  "name": "frontconsole",
  "version": "0.0.1",
  "description": "Super handy console tool for your web app. No external dependencies.",
  "main": "src/frontconsole.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "./node_modules/.bin/lessc --clean-css ./src/frontconsole.less ./build/frontconsole.css",
    "css:dist": "./node_modules/.bin/lessc --clean-css ./src/frontconsole.less ./dist/frontconsole.css",
    "build": "./node_modules/.bin/babel src -d build --source-maps && npm run css",
    "watch": "watch 'npm run build' ./src ./demo",
    "watch:dist": "watch 'npm run dist' ./src ./demo",
    "reload": "./node_modules/.bin/live-server --browser=chromium --watch=./build,./demo --open=./demo",
    "dist": "npm run css:dist && cp ./demo/dist.html ./dist/index.html && ./node_modules/.bin/webpack",
    "server": "./node_modules/.bin/webpack-dev-server"
  },
  "author": "zorzysty",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "live-server": "^1.1.0",
    "watch": "^1.0.1",
    "webpack-dev-server": "^1.16.2"
  }
}
