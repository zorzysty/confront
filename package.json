{
  "name": "frontconsole",
  "version": "0.0.1",
  "description": "Super handy console tool for your web app. No external dependencies.",
  "main": "src/frontconsole.js",
  "scripts": {
    "css": "./node_modules/.bin/lessc --clean-css ./src/frontconsole.less ./dist/frontconsole.css",
    "build": "npm run css && ./node_modules/.bin/webpack",
    "watch": "watch 'npm run build && npm run start' ./src ./demo",
    "start": "./node_modules/.bin/http-server"
  },
  "author": "zorza",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "http-server": "^0.9.0",
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "watch": "^1.0.1",
    "webpack": "^1.13.3"
  }
}
